maxin=max(maxin,in_17);



d0_18 = fk_18*input - (fa1_18*fd1_18 + fa0_18*fd2_18);
in_18 = a2d(max(abs(d0_18 - fd2_18),0.00001))+0.5;


out_18>in_18 ? (out_18=(out_18)-tr) : (out_18=out_18+ta);

peak_18>out_18 ?  (
    Chp_18>Cthp ? ( 
        peak_18=peak_18-Tr; 
        ) :
           Chp_18=Chp_18+1;):
                  (peak_18=out_18; Chp_18=0; (peak_18>0 ? peak_18=0;););

